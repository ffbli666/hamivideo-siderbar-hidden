{
    "manifest_version": 3,
    "name": "HamiVideo Slide Bar hidden",
    "description": "This extension hidden HamiVideo Slide Bar",
    "version": "1.0",
    "action": {
        "default_icon": {
            "19": "images/icon_24.png",
            "38": "images/icon_48.png"
        },
        "default_title": "HamiVideo Slide Bar hidden"
    },
    "icons": {
        "16" : "images/icon_24.png",
        "32" : "images/icon_48.png",
        "48" : "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "background": {
        "service_worker": "lib/background.js"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "declarativeContent"
    ],
    "host_permissions": [
        "*://hamivideo.hinet.net/*"
    ]
}